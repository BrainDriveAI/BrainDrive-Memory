# User Details
APP_USERNAME="your_app_username"

# Aura Neo4j
AURA_NEO4J_PWD="YOUR_AURA_NEO4J_PASSWORD"
NEO4J_URL="neo4j+s://YOUR_AURA_INSTANCE_ID.databases.neo4j.io"
NEO4J_USER="neo4j"
NEO4J_PWD="YOUR_NEO4J_PASSWORD"
NEO4J_DATABASE="neo4j"
AURA_INSTANCEID="YOUR_AURA_INSTANCE_ID"
AURA_INSTANCENAME="Your Aura Instance Name"

# LLM provider selection
LLM_PROVIDER=openai

# Embedding provider selection
EMBEDDING_PROVIDER=openai

# Feature toggles
ENABLE_AUTH=false
ENABLE_FILE_UPLOAD=false

# Agent Mode Selection
# Set to 'interviewer' for first-time setup/onboarding
# Set to 'memory' for regular personalized assistant usage
AGENT_MODE=interviewer
# =================================
# Quick Setup Instructions:
# 
# 1. FIRST TIME USERS:
#    - Set AGENT_MODE=interviewer
#    - Run the application for 1-2 hour onboarding interview
#    - This builds your initial knowledge base
#
# 2. REGULAR USAGE:
#    - Set AGENT_MODE=memory  
#    - Enjoy personalized AI assistant experience
#
# 3. TO SWITCH MODES:
#    - Update AGENT_MODE value above
#    - Restart the application
# =================================

# Google Oauth
GOOGLE_CLIENT_ID="YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="YOUR_GOOGLE_CLIENT_SECRET"
REDIRECT_URI="http://localhost:8501/"

# Google cloud storage
GCS_BUCKET_NAME="your_gcs_bucket_name"

# LLMSherpa
LLM_SHERPA_API_URL="http://localhost:5010/api/parseDocument?renderFormat=all&useNewIndentParser=true"

# Vertex AI Search
VERTEX_AI_PROJECT_ID="your-gcp-project-id"
VERTEX_AI_LOCATION_ID="us"
VERTEX_AI_SEARCH_ENGINE_ID="your_vertex_ai_search_engine_id"
VERTEX_AI_DATASTORE_ID="your_vertex_ai_datastore_id"

# PGVector
PG_CONNECTION_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"
PG_COLLECTION_NAME="my_memory"

# Pinecone
PINECONE_API_KEY="YOUR_PINECONE_API_KEY"
PINECONE_ENVIRONMENT="your_pinecone_environment" # e.g., us-west1-gcp
PINECONE_EMBEDDING_MODEL="multilingual-e5-large"

# Supabase
SUPABASE_URL="https://YOUR_PROJECT_REF.supabase.co"
SUPABASE_KEY="YOUR_SUPABASE_ANON_KEY"
SUPABASE_VECTOR_COLLECTION_NAME="documents_hybrid"
SUPABASE_VECTOR_QUERY_FUNCTION_NAME="hybrid_search"

# OpenAI
OPENAI_API_KEY="sk-YOUR_OPENAI_API_KEY"
OPENAI_LLM_MODEL="gpt-4.1"
OPENAI_EMBEDDING_MODEL="text-embedding-3-small"

# Groq
GROQ_API_KEY="gsk_YOUR_GROQ_API_KEY"
GROQ_LLM_MODEL="llama3-70b-8192"

# Ollama
OLLAMA_BASE_URL="http://localhost:11434"
OLLAMA_LLM_MODEL="llama3"
OLLAMA_EMBEDDING_MODEL="mxbai-embed-large"

# Openrouter
OPENROUTER_API_KEY="sk-or-YOUR_OPENROUTER_API_KEY"
OPENROUTER_BASE_URL="https://openrouter.ai/api/v1"
OPENROUTER_LLM_MODEL="meta-llama/llama-3.1-405b-instruct:free"

# TogetherAi
TOGETHER_AI_API_KEY="YOUR_TOGETHER_AI_API_KEY"
TOGETHER_AI_LLM_MODEL="meta-llama/Meta-Llama-3.1-405B-Instruct-Turbo"

# LangSmith
LANGSMITH_TRACING="false" # Set to "true" to enable LangSmith tracing
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY="lsv2_YOUR_LANGSMITH_API_KEY"
LANGSMITH_PROJECT="your_langsmith_project_name"

# Streamlit configuration
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false
STREAMLIT_GLOBAL_GATHER_USAGE_STATS=false
